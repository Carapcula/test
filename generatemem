prefix = "!"
emoji = "ü§£"
emoji2 = "üòÇ"
emojifinal = random.randint(1,2)
if emojifinal == 1:
    emojifinal = "ü§£"
if emojifinal == 2:
    emojifinal = "üòÇ"
