@bot.command()
async def eco(ctx, –≤–∏–¥ = None):
    trueword = False
    spisok = ["–ø–ª–∞—Å—Ç–∏–∫", "—Å—Ç–µ–∫–ª–æ", "–±—É–º–∞–≥–∞", "–∞–ª—é–º–∏–Ω–∏–π", "—Ä–µ–∑–∏–Ω–∞"]
    if –≤–∏–¥ == None:
        –≤–∏–¥ = random.choice(spisok)
    for word in spisok:
        if word == –≤–∏–¥:
            trueword = True
            break
    if trueword != True:
        await ctx.reply(f"–í—ã–±–µ—Ä–∏ –æ–¥–Ω–æ –∏–∑ –∑–Ω–∞—á–µ–Ω–∏–π! {spisok}")
    else:
        if –≤–∏–¥ == "—Ä–µ–∑–∏–Ω–∞":
            await ctx.reply("–†–µ–∑–∏–Ω–∞ —Ä–∞–∑–ª–∞–≥–∞–µ—Ç—Å—è `120`-`140` –ª–µ—Ç**!**")
        if –≤–∏–¥ == "–∞–ª—é–º–∏–Ω–∏–π":
            await ctx.reply("–ê–ª—é–º–∏–Ω–∏–π —Ä–∞–∑–ª–∞–≥–∞–µ—Ç—Å—è `500` –ª–µ—Ç**!**")
        if –≤–∏–¥ == "–±—É–º–∞–≥–∞":
            await ctx.reply("–ë—É–º–∞–≥–∞ —Ä–∞–∑–ª–∞–≥–∞–µ—Ç—Å—è –æ–∫–æ–ª–æ `2` –º–µ—Å—è—Ü–µ–≤**!**")
        if –≤–∏–¥ == "—Å—Ç–µ–∫–ª–æ":
            await ctx.reply("–°—Ç–µ–∫–ª–æ —Ä–∞–∑–ª–∞–≥–∞–µ—Ç—Å—è –æ–∫–æ–ª–æ `1000` –ª–µ—Ç**!**")
